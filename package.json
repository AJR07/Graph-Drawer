{
  "name": "graph-drawer",
  "version": "1.0.0",
  "description": "Graph Drawing given some edges and connected or not (supposed to help for CP)",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "npx cross-env NODE_ENV=development webpack serve",
    "build": "npx cross-env NODE_ENV=production webpack",
    "format": "npx prettier --write .",
    "test": "npm run test:lint && npm run test:format && npm run test:jest",
    "test:lint": "npx eslint .",
    "test:format": "npx prettier --check .",
    "test:jest": "npx jest",
    "postinstall": "npx is-ci || (npx husky install; npm run clear-webpack-cache)",
    "clear-webpack-cache": "npx rimraf node_modules/.cache/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AJR07/Graph-Drawer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJR07/Graph-Drawer/issues"
  },
  "homepage": "https://github.com/AJR07/Graph-Drawer",
  "dependencies": {
    "p5": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/p5": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "husky": "^5.1.3",
    "is-ci": "^2.0.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "pretty-format": "^26.6.2",
    "pretty-quick": "^3.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.2",
    "webpack": "^5.25.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
